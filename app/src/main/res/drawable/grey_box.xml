<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#ff31628b" />
            <stroke
                android:width="1dp"
                android:color="#ff101143" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ff8b8b8b"
                android:endColor="#ff434343"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ff434343" />
        </shape>
    </item>
    <item android:state_selected="true" >
        <shape>
            <solid
                android:color="#ff5cbf05" />
            <stroke
                android:width="1dp"
                android:color="#ff278900" />
        </shape>
    </item>

</selector>
